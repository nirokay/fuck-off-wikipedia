{
    "manifest_version": 3,
    "name": "Fuck Off Wikipedia",
    "description": "Removes Wikipedia donation notices and places a cookie as if you already donated.",
    "version": "0.1.0",

    "permissions": ["activeTab"],
    "content_scripts": [{
        "js": ["js/worker.js"],
        "matches": [
            "https://*.wikipedia.org/*",
            "http://*.wikipedia.org/*"
        ]
    }]
}
